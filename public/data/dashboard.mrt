        <Parameters isList="true" count="0" />
        <SqlCommand>declare @Ano smallint, @Mês tinyint;

set @Ano= year(getdate());
set @Mês= month(getdate());

declare @D1 date, @D2 date;
set dateformat dmy;
set @D1= cast ('1/' + cast (@Mês as varchar(2)) + '/' + cast (@Ano as char(4)) as date);
set @D2= dateadd (day, -1, dateadd (month, +1, @D1));

Select Distinct
	rel_faturamento.documento,
	rel_faturamento.empresa_id,
	rel_faturamento.empresa_razao_social,
	rel_faturamento.empresa_fantasia,
	rel_faturamento.status_nfe,
	rel_faturamento.pessoa_razao_social,
	rel_faturamento.pessoa_fantasia,
	rel_faturamento.valor_total_geral,
	rel_faturamento.valor_total_geral_produtos,
	rel_faturamento.vendedor_id,
	rel_faturamento.vendedor_fantasia,
	rel_faturamento.natureza_operacao,
	rel_faturamento.empresa_grupo_id,
	rel_faturamento.nr_documento,
	rel_faturamento.data_emissao,
	rel_faturamento.tipo_operacao_documento,
	rel_faturamento.municipio_nome,
	rel_faturamento.estado_nome,
	rel_faturamento.estado_uf,
	rel_faturamento.pessoa_vendedor_fantasia
	From
	rel_faturamento
	Where
	rel_faturamento.status_nfe = '100' And
	rel_faturamento.data_emissao Between @D1 And @D2
Order By
    rel_faturamento.documento</SqlCommand>
